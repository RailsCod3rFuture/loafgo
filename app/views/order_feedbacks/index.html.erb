<p id="notice"><%= notice %></p>

<h1>Order Feedbacks</h1>

<table>
  <thead>
  <tr>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @order_feedbacks.each do |order_feedback| %>
    <tr>
      <td><%= link_to 'Show', order_feedback %></td>
      <td><%= link_to 'Edit', edit_order_feedback_path(order_feedback) %></td>
      <td><%= link_to 'Destroy', order_feedback, method: :delete, data: {confirm: 'Are you sure?'} %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order Feedback', new_order_feedback_path %>
